<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AddBookInfoStageController">
   <bottom>
      <HBox alignment="CENTER" prefHeight="0.0" prefWidth="310.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#onAddBookInfoButtonClick" text="Add Book Info" />
         </children>
      </HBox>
   </bottom>
   <center>
      <GridPane prefHeight="105.0" prefWidth="387.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="190.0" minWidth="10.0" prefWidth="133.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="299.0" minWidth="10.0" prefWidth="254.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label prefHeight="17.0" prefWidth="118.0" text="SECURITIES CODE" />
            <Label text="BOOK NAME" GridPane.rowIndex="1" />
            <Label text="MARKET" GridPane.rowIndex="2" />
            <TextField fx:id="securitiesCodeText" prefHeight="25.0" prefWidth="254.0" GridPane.columnIndex="1" />
            <TextField fx:id="bookNameText" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="marcketText" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </center>
   <top>
      <Label text="Add Book Name" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
